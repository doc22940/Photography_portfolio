ction init() {\n  cacheDom();\n  bindEvents();\n  initRadioBtn();\n}\n\nconst module = { init };\nexport default module;\n"},"lineCount":null}},"error":null,"hash":"cf21a627787f58f9a46fffa9aae7c853","cacheData":{"env":{}}}